{
  "name": "@invisionapp/dsm-storybook",
  "main": "dist/dsm-storybook.js",
  "typings": "dsm-storybook.d.ts",
  "version": "0.0.93",
  "repository": {
    "type": "git",
    "url": "https://github.com/InVisionApp/dsm-storybook"
  },
  "scripts": {
    "prepublishOnly": "npm run build-prod",
    "build-prod": "rimraf ./dist && npm run build-prod-web && npm run build-prod-node",
    "build-dev": "rimraf ./dist && npm run build-dev-web && npm run build-dev-node",
    "build-prod-web": "webpack --mode production --config ./config/webpack.config.base.js",
    "build-dev-web": "webpack --mode development --config ./config/webpack.config.dev.js",
    "build-prod-node": "webpack --mode production --config ./config/webpack-node.config.base.js",
    "build-dev-node": "webpack --mode development --config ./config/webpack-node.config.dev.js",
    "start": "webpack --mode development --watch --config ./config/webpack.config.dev.js & webpack --mode development --watch --config ./config/webpack-node.config.dev.js",
    "install-test-dsm-storybook": "node ./test/system-tests/scripts/install-dsm-storybook.js",
    "fetch-test-example-libraries": "node ./test/system-tests/scripts/fetch-example-libraries.js",
    "publish-test-example-libraries": "node ./test/system-tests/scripts/publish-example-libraries-stories.js",
    "start-cypress": "node ./test/system-tests/scripts/start-cypress.js",
    "start-cypress-interactive": "DSM_TEST_CYPRESS_INTERACTIVE=true node ./test/system-tests/scripts/start-cypress.js",
    "test": "NODE_ENV=test jest --config jest.config.json",
    "test-local": "docker-compose -f docker-compose.yml -f docker-compose-test.yml up",
    "test-system-local": "npm run dc-build-system-test && npm run dc-prepare-system-test && npm run dc-start-system-test",
    "prepare-system-test": "npm run fetch-test-example-libraries && npm run install-test-dsm-storybook && npm run publish-test-example-libraries",
    "dc-build-system-test": "docker-compose -f docker-compose.yml -f docker-compose-system-test.yml -f docker-compose-prepare-system-test.yml build",
    "dc-prepare-system-test": "docker-compose --log-level INFO -f docker-compose.yml -f docker-compose-prepare-system-test.yml up",
    "dc-start-system-test": "docker-compose -f docker-compose-system-test.yml up",
    "test:coverage": "npm run test && npm run --silent upload-coverage",
    "upload-coverage": "[ \"$CI_NAME\" == \"codeship\" ] && codecov || echo \"Skipping codecov upload - Will only upload when running on Codeship\"",
    "lint": "eslint src",
    "local-publish": "yalc publish --no-sig --force",
    "clean-yalc": "yalc installations clean @invisionapp/dsm-storybook",
    "postupdate": "chmod +x ./node_modules/.bin/dsm-storybook",
    "sanity": "./sanity.sh"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "UNLICENSED",
  "bin": {
    "dsm-storybook": "bin/dsm-storybook.js"
  },
  "files": [
    "dist",
    "bin",
    "backend-utils.js",
    "register.js",
    "dsm-storybook.d.ts"
  ],
  "dependencies": {
    "@angular/compiler": "7.1.2",
    "@babel/traverse": "7.1.6",
    "@babel/types": "7.2.2",
    "angular-docgen": "1.3.1",
    "axios": "0.19.0",
    "chalk": "2.4.1",
    "cli-table3": "0.5.1",
    "codecov": "3.3.0",
    "commander": "2.19.0",
    "core-js": "2.6.5",
    "cosmiconfig": "5.0.7",
    "fs-extra": "7.0.1",
    "get-port": "4.2.0",
    "glob": "7.1.3",
    "lodash": "4.17.11",
    "mime-types": "2.1.21",
    "moment": "2.24.0",
    "ora": "3.4.0",
    "pkg-dir": "4.1.0",
    "prettier": "1.17.1",
    "prismjs": "1.15.0",
    "react-docgen": "5.0.0-beta.1",
    "read-pkg": "4.0.1",
    "read-pkg-up": "4.0.0",
    "rimraf": "2.6.2",
    "semver": "6.0.0",
    "shelljs": "0.8.3",
    "shortid": "2.2.14",
    "tunnel": "0.0.6",
    "cross-env": "5.2.0",
    "url-parse": "1.4.4",
    "vue-docgen-api": "3.11.4",
    "vue-template-compiler": "2.6.10"
  },
  "peerDependencies": {
    "@storybook/addon-options": ">= 4.1.11 < ^5.0.10",
    "@storybook/addons": ">= 4.1.11 < ^5.0.10",
    "react": "*"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@invisionapp/eslint-config-dsm": "latest",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-prismjs": "1.0.2",
    "bluebird": "3.5.5",
    "chai": "4.2.0",
    "child_process": "1.0.2",
    "css-loader": "2.0.2",
    "deep-equal-in-any-order": "1.0.13",
    "eslint": "5.6.0",
    "eslint-plugin-react": "7.11.1",
    "express": "4.17.1",
    "jest": "24.7.1",
    "raw-loader": "2.0.0",
    "replace-in-file": "4.1.1",
    "style-loader": "0.23.1",
    "superagent": "5.1.0",
    "unzipper": "0.10.1",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0",
    "yalc": "1.0.0-pre.27"
  }
}